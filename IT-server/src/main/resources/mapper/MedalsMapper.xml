<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xiaoyan.mapper.MedalsMapper">

    <select id="selectByStudentId" resultType="com.xiaoyan.pojo.StudentMedals">
        select *
        from student_medals
        <where>
            <if test="studentId!=null">
                student_id=#{studentId}
            </if>
        </where>
    </select>
</mapper>